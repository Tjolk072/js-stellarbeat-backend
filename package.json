{
  "name": "@stellarbeat/js-stellarbeat-backend",
  "version": "0.2.0",
  "description": "Backend & api",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarbeat/js-stellarbeat-backend.git"
  },
  "scripts": {
    "build": "tsc --declaration",
    "db:migrate:make": "knex migrate:make",
    "db:migrate:latest": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:seed:run": "knex seed:run",
    "test": "jest",
    "heroku-postbuild": "npm run flow:build",
    "build-and-update-nodes": "npm run build; node lib/commands/update-nodes.js",
    "update-nodes": "node lib/commands/update-nodes",
    "build-and-start-api": "npm run flow:build; node lib/index.js",
    "start": "node lib/index.js"
  },
  "author": "pieterjan84@github",
  "license": "MIT",
  "main": "lib/index.js",
  "dependencies": {
    "@stellarbeat/js-stellar-domain": "github:stellarbeat/js-stellar-domain#master",
    "@stellarbeat/js-stellar-node-crawler": "github:stellarbeat/js-stellar-node-crawler#master",
    "await-fs": "^1.0.0",
    "aws-sdk": "^2.368.0",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "knex": "^0.15",
    "lodash": "^4.17.10",
    "npm": "^6.1.0",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/knex": "0.15",
    "@types/node": "^10.12.12",
    "@types/swagger-ui-express": "^3.0.0",
    "jest": "^23.6.0",
    "merge": ">=1.2.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  }
}
