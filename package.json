    "aws-sdk": "^2.368.0",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "knex": "^0.15",
    "lodash": "^4.17.10",
    "npm": "^6.1.0",
    "swagger-ui-express": "^4.0.1",
    "toml": "^3.0.0"
